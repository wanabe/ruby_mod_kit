# frozen_string_literal: true

module RubyModKit
  # the base class of feature
  class Feature
    def <=>(Feature => other): (-1 | 0 | 1 | nil)
      self.class.name <=> other.class.name
    end

    def create_correctors: (Array[Corrector])
      []
    end

    def create_missions: (Array[Mission])
      []
    end
  end
end
