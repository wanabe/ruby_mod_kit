# frozen_string_literal: true

# rbs_inline: enabled

module RubyModKit
  module Node
    # Transpiler program node
    class StatementsNode < Node::BaseNode
      getter @prism_node: Prism::StatementsNode
      getter @parent: (Node::BaseNode)

      def initialize(@prism_node, @parent:): void
        raise RubyModKit::Error unless prism_node.is_a?(Prism::StatementsNode)

        super()
      end
    end
  end
end
