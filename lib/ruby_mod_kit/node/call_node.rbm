# frozen_string_literal: true

# rbs_inline: enabled

module RubyModKit
  class Node
    # Transpiler method call node
    class CallNode < Node
      getter @prism_node: Prism::CallNode
      getter @name: Symbol
      getter @parent: Node

      def initialize(@prism_node, @parent:): void
        raise RubyModKit::Error unless prism_node.is_a?(Prism::CallNode)

        @name = prism_node.name.to_sym
        super()
      end
    end
  end
end
