# frozen_string_literal: true

# rbs_inline: enabled

module RubyModKit
  module Memo
    # The base class for located memo
    class OffsetMemo
      getter @offset: Integer

      def initialize(Integer => offset): void
        @offset = offset
      end

      def succ(OffsetDiff => offset_diff): void
        @offset = offset_diff[@offset]
      end
    end
  end
end
