# frozen_string_literal: true

# rbs_inline: enabled

module RubyModKit
  class Memo
    # The memo for parameter type
    class Parameter < NodeMemo
      attr_reader :type #: String

      def initialize(Node::ParameterNode => node, String => @type): void
        @name = node.name
        super(node)
      end
    end
  end
end
