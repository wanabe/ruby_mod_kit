# frozen_string_literal: true

# rbs_inline: enabled

module RubyModKit
  class Memo
    # The memo for parameter type
    class Ivar
      @type: String

      attr_reader :type

      def initialize(Symbol => @name): void
      end

      def type=(String => type): void
        @type = Memo.unify_type(type)
      end
    end
  end
end
